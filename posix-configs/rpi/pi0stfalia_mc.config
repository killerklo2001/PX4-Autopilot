#!/bin/sh
# PX4 commands need the 'px4-' prefix in bash.
# (px4-alias.sh is expected to be in the PATH)
. px4-alias.sh

param select parameters.bson
param import

# system_power not implemented
param set CBRK_SUPPLY_CHK 894281

param set-default BAT1_V_DIV 7.7

# broadcast to LAN
# always keep current config
param set SYS_AUTOCONFIG 0
# useless but required for parameter completeness
param set MAV_TYPE 1
param set SYS_AUTOSTART 2100

dataman start -r

load_mon start

battery_status start

# internal IMU
bno085 start -s -R 9 -f 2000 # Rotationtable in /src/lib/conversion/rotation.h - 9 is ROTATION_ROLL_180_YAW_45
bno085 status

#baro
dps310 start -I
dps310 status

# ADC
ads1115 start -I
ads1115 status

# PWM
pca9685_pwm_out start -b 0
control_allocator start

rc_input start -d /dev/ttyAMA0

rc_update start
sensors start
commander start
navigator start
ekf2 start
airspeed_selector start
land_detector start multicopter
flight_mode_manager start
fw_att_control start
fw_mode_manager start
fw_lat_lon_control start

# Telem
mavlink start -u 14556 -r 1000000
mavlink stream -u 14556 -s HIGHRES_IMU -r 50

#logger start -t -b 200

mavlink boot_complete
